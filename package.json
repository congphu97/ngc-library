{
  "name": "cubable",
  "version": "1.16.1",
  "description": "Cubable Frontend",
  "license": "MIT",
  "authors": "Cubable Team",
  "private": true,
  "scripts": {
    "ng": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng",
    "sass-lint": "node --max-old-space-size=4096 ./node_modules/sass-lint/bin/sass-lint.js",
    "version:patch": "npm --no-git-tag-version version patch && node store-version.js",
    "start": "ng serve --host localhost",
    "aot": "ng serve --host localhost --configuration=aot",
    "lite": "ng serve --host localhost --configuration=lite",
    "local": "ng serve --host localhost --configuration=local",
    "sw": "ng build --configuration=sw && http-server -p 8000 -c-1 ./dist_sw",
    "deployment:build": "npm i && ng build --configuration=deployment --output-path ./dist/source",
    "testing:build": "npm i && ng build --configuration=testing --output-path ./dist/source",
    "staging:build": "npm i && ng build --configuration=staging --output-path ./dist/source",
    "production:build": "npm i && ng build --configuration=production --output-path ./dist/source",
    "lint": "ng lint && sass-lint -c .sass-lint.yml -v -q && pug-lint src"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@ctrl/ngx-emoji-mart": "^6.2.0",
    "@ngx-loading-bar/core": "^5.1.1",
    "@ngx-loading-bar/http-client": "^5.1.1",
    "angular-core": "git+https://github.com/congphu97/ngc-core.git#v1.2",
    "angular-resizable-element": "^3.3.2",
    "angular-svg-round-progressbar": "^8.0.0",
    "chart.js": "^3.1.1",
    "cropperjs": "^1.3.3",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "html-to-image": "^0.1.1",
    "image-conversion": "^2.1.1",
    "libphonenumber-js": "^1.7.52",
    "lodash": "4.17.15",
    "ng-lazyload-image": "9.1.3",
    "ng-recaptcha": "^9.0.0",
    "ngx-color": "^5.1.3",
    "ngx-quill": "^15.0.0",
    "ngx-quill-upload": "^2.0.0",
    "quill": "^1.3.7",
    "quill-magic-url": "^2.0.2",
    "quill-mention": "^3.0.0",
    "quill-table": "^1.0.0",
    "quill-table-ui": "^1.0.7",
    "resize-observer-polyfill": "^1.5.1",
    "tslib": "^2.4.0",
    "xlsx": "^0.16.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-devkit/core": "^13.3.5",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/language-service": "13.3.8",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jquery": "^3.3.30",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "apply-loader": "^2.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^3.0.8",
    "ng-cli-pug-loader": "^0.2.2",
    "sass-lint": "^1.12.1",
    "ts-node": "~3.2.0",
    "typescript": "4.6.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
