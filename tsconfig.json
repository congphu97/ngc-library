{
  "extends": "./node_modules/angular-core/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@app": [ "app" ],
      "@app/*": [ "app/*" ],
      "@main": [ "app/main" ],
      "@main/*": [ "app/main/*" ],
      "@resources": [ "app/@resources" ],
      "@resources/*": [ "app/@resources/*" ],
      "@core": [ "app/@core" ],
      "@core/*": [ "app/@core/*" ],
      "@error": [ "app/@error" ],
      "@error/*": [ "app/@error/*" ],
      "@wgc": [ "app/@wgc" ],
      "@wgc/*": [ "app/@wgc/*", "app/@wgc/lib/*" ],
      "@environments": [ "environments" ],
      "@environments/*": [ "environments/*" ]
    }
  }
}
